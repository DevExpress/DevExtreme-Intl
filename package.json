  ],
  "peerDependencies": {
    "devextreme": "17.1 - 19.1.7"
  },
  "devDependencies": {
    "cldr-core": "latest",
    "cldrjs": "^0.3.1",
    "devextreme": "17.1 - 19.1.7",
    "eslint": "^3.10.2",
    "intl": "^1.2.5",
    "jquery": "^3.0.0",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-junit-reporter": "^1.1.0",
    "karma-qunit": "^1.2.1",
    "karma-webpack": "^1.8.0",
    "npm-run-all": "^4.1.3",
    "object-assign": "^4.1.0",
    "parallelshell": "^2.0.0",
    "puppeteer": "^1.7.0",
    "qunitjs": "^1.23.1",
    "systemjs": "^0.19.41",
    "systemjs-plugin-json": "^0.2.2",
    "webpack": "^1.13.3",
    "yargs": "^6.4.0"
  },
  "scripts": {
    "prepare-build": "node scripts/locale-data-generator.js",
    "build": "npm run prepare-build -s && webpack && webpack --min",
