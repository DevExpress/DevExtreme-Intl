{
  "name": "devextreme-intl",
  "description": "Integrates Intl with DevExtreme 16.2",
  "version": "16.2.1",
  "author": "Developer Express Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/DevExpress/DevExtreme-Intl.git"
  },
  "dependencies": {
    "devextreme": "16.2-unstable"
  },
  "devDependencies": {
    "cldr-core": "^30.0.2",
    "cldrjs": "^0.3.1",
    "intl": "^1.2.5",
    "jquery": "^2.0.0 || ^3.0.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-qunit": "^0.1.9",
    "parallelshell": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.7",
    "qunitjs": "^1.23.1",
    "systemjs": "^0.19.41",
    "webpack": "^1.13.3",
    "yargs": "^6.4.0"
  },
  "scripts": {
    "prepare-build": "node scripts/locale-data-generator.js",
    "build": "npm run prepare-build -s && webpack && webpack --min",
    "build-dev": "npm run prepare-build -s && webpack --dev",
    "test-dev": "parallelshell \"npm run build-dev -s\" \"karma start --browsers Chrome\"",
    "test-dev-phantom": "parallelshell \"npm run build-dev -s\" \"karma start --browsers PhantomJS\"",
    "test": "npm run build && karma start --browsers PhantomJS --single-run --use-prod-bundle=true"
  },
  "keywords": [
    "intl",
    "devextreme",
    "devexpress"
  ]
}
